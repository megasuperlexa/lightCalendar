<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>LightCalendar</PackageId>
    <PackageDescription>Lightweight .Net library for time intervals</PackageDescription>
    <PackageTags>time;periods;intervals;dates</PackageTags>
    <RepositoryUrl>https://github.com/megasuperlexa/lightCalendar</RepositoryUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Version>1.0.0</Version>
    <Authors>Aliaksei Sosin</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.7.0" />
  </ItemGroup>

</Project>
